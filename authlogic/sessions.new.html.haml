#login
  = render_flash

  - semantic_form_for @user_session, :url => user_session_path do |f|
    - f.inline_errors = :none
    - f.inputs do
      = f.input :email
      = f.input :password
      = f.input :remember_me, :as => :boolean, :label => 'Remember Me', :required => false
      = f.commit_button 'Login'
