<dl>
	<dt>Email:</dt>
	<dd><%= h @user.email %></dd>

	<dt>Login count:</dt>
	<dd><%= h @user.login_count %></dd>

	<dt>Last request at:</dt>
	<dd><%= h @user.last_request_at %></dd>

	<dt>Last login at:</dt>
	<dd><%= h @user.last_login_at ||= 'No Data.' %></dd>

	<dt>Current login at:</dt>
	<dd><%= h @user.current_login_at %></dd>

	<dt>Last login ip:</dt>
	<dd><%= h @user.last_login_ip ||= 'No Data.' %></dd>

	<dt>Current login ip:</dt>
	<dd><%= h @user.current_login_ip %></dd>
</dl>
<%= link_to 'Edit', edit_user_path %> | <%= link_to "Back to User List", users_path %>